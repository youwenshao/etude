# Database Configuration
POSTGRES_DB=etude_dev
POSTGRES_USER=etude
POSTGRES_PASSWORD=etude_dev_password
DATABASE_URL=postgresql+asyncpg://etude:etude_dev_password@postgres:5432/etude_dev

# MinIO/S3 Configuration
MINIO_ENDPOINT=minio:9000
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin123
MINIO_USE_SSL=false
MINIO_BUCKET_ARTIFACTS=etude-artifacts
